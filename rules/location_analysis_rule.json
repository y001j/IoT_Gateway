{
  "id": "location_analysis_rule",
  "name": "GPS位置数据分析",
  "description": "分析GPS位置数据并计算移动距离",
  "enabled": true,
  "priority": 90,
  "version": 9,
  "conditions": {
    "type": "simple",
    "field": "key",
    "operator": "eq",
    "value": "location"
  },
  "actions": [
    {
      "type": "transform",
      "config": {
        "add_tags": {
          "location_processed": "true"
        },
        "field": "value.latitude",
        "output_key": "rounded_lat",
        "precision": 6,
        "type": "round"
      }
    },
    {
      "type": "alert",
      "config": {
        "level": "warning",
        "message": "设备{{.DeviceID}}移动速度过快: {{.value.speed}}km/h"
      }
    }
  ],
  "created_at": "2025-08-14T21:46:15.558499+08:00",
  "updated_at": "2025-08-15T19:50:27.3206786+08:00"
}