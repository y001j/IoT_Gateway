{
  "id": "vibration_performance_monitor",
  "name": "振动性能监控聚合",
  "description": "监控振动传感器性能指标，包括设备健康度分析",
  "enabled": true,
  "priority": 55,
  "version": 1,
  "conditions": {
    "type": "and",
    "and": [
      {
        "type": "simple",
        "field": "device_id",
        "operator": "eq",
        "value": "vibration_sensor_01"
      },
      {
        "type": "or",
        "or": [
          {
            "type": "simple",
            "field": "key",
            "operator": "eq",
            "value": "vibration"
          },
          {
            "type": "simple",
            "field": "key",
            "operator": "eq",
            "value": "rotation_speed"
          }
        ]
      }
    ]
  },
  "actions": [
    {
      "type": "aggregate",
      "config": {
        "window_type": "time",
        "window": "5m",
        "functions": [
          "count", "avg", "max", "min",
          "above_count", "below_count", "in_range_count",
          "cv", "volatility"
        ],
        "thresholds": {
          "above": 8,
          "below": 2,
          "min_range": 2,
          "max_range": 8
        },
        "group_by": ["device_id"],
        "output_key": "equipment_health",
        "add_tags": {
          "monitor_type": "vibration_health",
          "analysis_period": "5min_cumulative",
          "health_model": "v2.1"
        }
      }
    }
  ],
  "created_at": "2025-08-10T22:00:00Z",
  "updated_at": "2025-08-10T22:00:00Z"
}