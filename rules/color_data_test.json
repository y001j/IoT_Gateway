{
  "id": "color_data_test",
  "name": "颜色数据处理测试",
  "description": "测试颜色复合数据的处理和转换",
  "enabled": false,
  "priority": 50,
  "version": 7,
  "data_type": "color",
  "conditions": {
    "type": "and",
    "and": [
      {
        "type": "simple",
        "field": "device_id",
        "operator": "eq",
        "value": "color_device_01"
      },
      {
        "type": "simple",
        "field": "key",
        "operator": "eq",
        "value": "color"
      }
    ]
  },
  "actions": [
    {
      "type": "alert",
      "config": {
        "level": "info",
        "message": "颜色数据: R={{color.r}}, G={{color.g}}, B={{color.b}}, 色相={{color.hue}}°, 饱和度={{color.saturation}}, 亮度={{color.lightness}}",
        "channels": ["console"]
      }
    },
    {
      "type": "transform",
      "config": {
        "type": "expression",
        "expression": "value.hue",
        "output_key": "color_hue",
        "add_tags": {
          "color_property": "hue",
          "extracted_from": "color_data",
          "unit": "degrees"
        }
      }
    }
  ],
  "created_at": "2025-08-12T21:46:32.6056361+08:00",
  "updated_at": "2025-08-12T23:45:17.7100669+08:00"
}