{
  "id": "vector_statistics_rule",
  "name": "向量数据统计分析",
  "description": "对向量数据进行统计分析",
  "enabled": true,
  "priority": 65,
  "version": 9,
  "conditions": {
    "type": "simple",
    "field": "key",
    "operator": "eq",
    "value": "signal_vector"
  },
  "actions": [
    {
      "type": "aggregate",
      "config": {
        "forward": true,
        "functions": [
          "avg",
          "max",
          "min",
          "stddev"
        ],
        "group_by": [
          "device_id"
        ],
        "output_key": "vector_stats",
        "size": 10,
        "window_type": "count"
      }
    },
    {
      "type": "transform",
      "config": {
        "add_tags": {
          "vector_processed": "true"
        },
        "field": "value.values",
        "precision": 2,
        "type": "round"
      }
    }
  ],
  "created_at": "2025-08-14T21:46:15.5754084+08:00",
  "updated_at": "2025-08-15T19:50:27.3357118+08:00"
}