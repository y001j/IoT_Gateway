{
  "id": "temperature_statistics_aggregate",
  "name": "温度统计聚合",
  "description": "计算温度数据的统计指标，包括基础统计和高级分析",
  "enabled": true,
  "priority": 50,
  "version": 1,
  "conditions": {
    "type": "and",
    "and": [
      {
        "type": "simple",
        "field": "key",
        "operator": "eq",
        "value": "temperature"
      },
      {
        "type": "simple",
        "field": "device_id",
        "operator": "startswith",
        "value": "temp_sensor"
      }
    ]
  },
  "actions": [
    {
      "type": "aggregate",
      "config": {
        "window_type": "count",
        "size": 10,
        "functions": [
          "count", "avg", "min", "max", "stddev"
        ],
        "output_key": "temp_stats",
        "group_by": ["device_id"],
        "forward": true,
        "add_tags": {
          "aggregation_type": "temperature_statistics",
          "window_type": "sliding_10_count",
          "analysis_level": "basic"
        }
      }
    }
  ],
  "created_at": "2025-08-10T22:00:00Z",
  "updated_at": "2025-08-10T22:00:00Z"
}